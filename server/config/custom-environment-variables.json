{
  "firestore": {
    "host": "FIRESTORE_HOST",
    "port": "FIRESTORE_PORT"
  },
  "pubsub": {
    "client": {
      "apiEndpoint": "PUBSUB_API_ENDPOINT"
    },
    "pushEndpointBase": "PUBSUB_PUSH_ENDPOINT_BASE"
  },
  "providers": {
    "file": {
      "gcloud": {
        "translationsBucket": "GCS_TRANSLATIONS_BUCKET",
        "templatesBucket": "GCS_TEMPLATES_BUCKET",
        "documentsBucket": "GCS_DOCUMENTS_BUCKET",
        "tempBucket": "GCS_TEMP_BUCKET"
      }
    },
    "email": {
      "smtp": {
        "host": "SMTP_HOST",
        "port": { "__name": "SMTP_PORT", "__format": "number" },
        "authUsername": "SMTP_USERNAME",
        "authPassword": "SMTP_PASSWORD",
        "from": "EMAIL_FROM",
        "replyTo": "EMAIL_REPLY_TO"
      }
    },
    "authentication": {
      "jwt": {
        "jwksUrl": "AUTH_JWT_JWKS_URL",
        "audience": "AUTH_JWT_AUDIENCE",
        "issuer": "AUTH_JWT_ISSUER"
      }
    }
  },
  "cors": {
    "enabled": {
      "__name": "CORS_ENABLED",
      "__format": "boolean"
    },
    "allowedOrigins": {
      "__name": "CORS_ALLOWED_ORIGINS",
      "__format": "json"
    }
  },
  "correspondence": {
    "enabled": "CORR_ENABLED",
    "no-mailing-addr": {
      "subject": "CORR_NO_MAILING_ADDR_SUBJECT"
    },
    "reminder-mailing-addr": {
      "subject": "CORR_REMINDER_MAILING_ADDR_SUBJECT"
    },
    "thank-you": {
      "subject": "CORR_THANK_YOU_SUBJECT"
    }
  }
}
